  "scripts": {
    "lint": "prettier --write \"**/*{js,json,ts,tsx,json}\"",
    "prettier-check": "prettier --list-different \"**/*{js,json,ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "type-coverage": "type-coverage --strict --at-least 85",
    "codegen:admin": "apollo client:codegen --customScalarsPrefix hasura_ --passthroughCustomScalars --config=src/lib/graphql/roles/admin/apollo.config.js --target=typescript --tagName=gql --outputFlat src/lib/graphql/roles/admin/generated && prettier --write src/lib/graphql/roles/admin/generated/**/*.ts",
    "codegen:developer": "apollo client:codegen --customScalarsPrefix hasura_ --passthroughCustomScalars --config=src/lib/graphql/roles/developer/apollo.config.js --target=typescript --tagName=gql --outputFlat src/lib/graphql/roles/developer/generated && prettier --write src/lib/graphql/roles/developer/generated/**/*.ts",
    "codegen:client": "apollo client:codegen --customScalarsPrefix hasura_ --passthroughCustomScalars --config=src/lib/graphql/roles/client/apollo.config.js --target=typescript --tagName=gql --outputFlat src/lib/graphql/roles/client/generated && prettier --write src/lib/graphql/roles/client/generated/**/*.ts",
    "codegen:user": "apollo client:codegen --customScalarsPrefix hasura_ --passthroughCustomScalars --config=src/lib/graphql/roles/user/apollo.config.js --target=typescript --tagName=gql --outputFlat src/lib/graphql/roles/user/generated && prettier --write src/lib/graphql/roles/user/generated/**/*.ts",
    "codegen:agency": "apollo client:codegen --customScalarsPrefix hasura_ --passthroughCustomScalars --config=src/lib/graphql/roles/agency/apollo.config.js --target=typescript --tagName=gql --outputFlat src/lib/graphql/roles/agency/generated && prettier --write src/lib/graphql/roles/agency/generated/**/*.ts",
    "codegen": "yarn run codegen:developer && yarn run codegen:client && yarn run codegen:user && yarn run codegen:agency && yarn run codegen:admin",
    "codegen:clean": "rm -rf src/lib/graphql/roles/developer/generated && rm -rf src/lib/graphql/roles/client/generated && rm -rf src/lib/graphql/roles/user/generated && rm -rf src/lib/graphql/roles/agency/generated && rm -rf src/lib/graphql/roles/admin/generated && yarn run codegen",
    "dev": "next",
    "build": "cross-env-shell SENTRY_DSN=https://ce28a73937124a1ea6fa0ee17dcf9be9@sentry.io/1817709 next build",
    "start": "cross-env NODE_PATH=dist SENTRY_DSN=https://ce28a73937124a1ea6fa0ee17dcf9be9@sentry.io/1817709 next start -p $PORT",
    "storybook": "start-storybook",
    "dedupe": "yarn run yarn-deduplicate && yarn install",
    "dedupe-check": "yarn run yarn-deduplicate --fail",
    "build-storybook": "build-storybook -s storybook-static",
    "test": "jest -c jest.config.js -i"
  },
